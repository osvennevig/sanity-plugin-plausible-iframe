{
  "name": "sanity-plugin-plausible-iframe",
  "version": "1.0.1",
  "description": "Simple plugin to show Plausible analytics as an iframe inside of the Sanity Dashboard.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "prepublishOnly": "sanipack build && sanipack verify"
  },
  "repository": {},
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Ole Svennevig",
  "license": "MIT",
  "dependencies": {
    "@sanity/dashboard": "^2.21.10"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-react": "^7.27.0",
    "prettier": "^2.4.1",
    "sanipack": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier"
    ],
    "ignorePatterns": [
      "lib/**/"
    ]
  }
}
